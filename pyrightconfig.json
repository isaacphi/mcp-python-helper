{
  "include": ["."],
  "exclude": ["**/node_modules", "**/__pycache__", ".venv"],
  "pythonVersion": "3.10",
  "pythonPlatform": "Darwin",
  "venvPath": ".",
  "venv": ".venv",
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": true,
  "enableWorkspaceSymbols": true,
  "reportUnusedImport": false,
  "reportUnusedVariable": false
}
